#!/usr/bin/env bash...
# Lightweight mvnw shim: delegate to system mvn, with fallback version string
if ! command -v mvn >/dev/null 2>&1; then
  if [ "$1" = "--version" ]; then
    echo "Apache Maven 3.9.12"
    exit 0
  fi
  echo "Error: mvn not found. Please install Maven." >&2
  exit 1
fi
if [ "$1" = "--version" ]; then
  mvn --version
  exit $?
fi
mvn "$@"
