# Server
PORT=8080

# Frontend URL allowed by backend
FRONTEND_BASE_URL=https://YOUR_FRONTEND_URL

# Cloud SQL
DB_DRIVER=com.mysql.cj.jdbc.Driver
DB_URL=jdbc:mysql://YOUR_DB_HOST:3306/ratelimiter_db?useSSL=true&requireSSL=true&serverTimezone=UTC
DB_USERNAME=YOUR_DB_USER
DB_PASSWORD=YOUR_DB_PASSWORD

# Cloud Redis
REDIS_HOST=YOUR_REDIS_HOST
REDIS_PORT=6379
REDIS_USERNAME=
REDIS_PASSWORD=YOUR_REDIS_PASSWORD
REDIS_SSL_ENABLED=true

# Session cookie
SESSION_COOKIE_SECURE=true
SESSION_COOKIE_DOMAIN=YOUR_DOMAIN
