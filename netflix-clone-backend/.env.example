# Server
PORT=8082

# Cloud SQL (Neon PostgreSQL example)
DB_DRIVER=org.postgresql.Driver
DB_URL=jdbc:postgresql://YOUR_NEON_HOST/neondb?sslmode=require&channel_binding=require
DB_USERNAME=neondb_owner
DB_PASSWORD=YOUR_DB_PASSWORD

# Cloud Redis
REDIS_HOST=YOUR_REDIS_HOST
REDIS_PORT=6379
REDIS_USERNAME=
REDIS_PASSWORD=YOUR_REDIS_PASSWORD
REDIS_SSL_ENABLED=true

# App integration
RATELIMITER_BASE_URL=https://YOUR_DISTRIBUTED_RATELIMITER_BACKEND

# Session cookie (set secure=true and a real domain in production)
SESSION_COOKIE_SAMESITE=None
SESSION_COOKIE_SECURE=true
SESSION_COOKIE_DOMAIN=YOUR_DOMAIN
